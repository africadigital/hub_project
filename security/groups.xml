<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">

    <record model="ir.module.category" id="module_category_hub_project">
        <field name="name">Hub Projet</field>
        <field name="description">Aide pour le choix des droits d'acces.</field>
        <field name="sequence">10</field>
    </record>

    <record id="group_hub_project_user" model="res.groups">
        <field name="name">Demandeur Fiche projet</field>
        <field name="category_id" ref="module_category_hub_project"/>
    </record>

    <record id="group_chief_project" model="res.groups">
        <field name="name">Chef projet</field>
        <field name="category_id" ref="module_category_hub_project"/>
        <field name="implied_ids" eval="[(4, ref('hub_project.group_hub_project_user'))]"/>
    </record>

    <record id="group_hub_pmo" model="res.groups">
        <field name="name">PMO</field>
        <field name="category_id" ref="module_category_hub_project"/>
         <field name="implied_ids" eval="[(4, ref('hub_project.group_hub_project_user'))]"/>
    </record>
    <record id="group_hub_dsd" model="res.groups">
        <field name="name">DSD</field>
        <field name="category_id" ref="module_category_hub_project"/>
         <field name="implied_ids" eval="[(4, ref('hub_project.group_hub_project_user'))]"/>
    </record>


    <record id="group_hub_manager" model="res.groups">
        <field name="name">Directeur Général</field>
        <field name="category_id" ref="module_category_hub_project"/>
        <field name="implied_ids" eval="[(4, ref('hub_project.group_chief_project')),(4, ref('hub_project.group_hub_pmo')),(4, ref('hub_project.group_hub_dsd'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

